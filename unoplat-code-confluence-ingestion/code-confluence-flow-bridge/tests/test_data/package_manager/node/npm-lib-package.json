{
  "name": "@unoplat/example-library",
  "version": "2.3.1",
  "description": "A comprehensive test library package for validating package.json parsing with TypeScript support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "typesVersions": {
    ">=4.0": {
      "*": [
        "dist/*"
      ]
    }
  },
  "bin": {
    "unoplat-example-library": "./bin/cli.js"
  },
  "keywords": [
    "typescript",
    "library",
    "utility",
    "package-manager"
  ],
  "author": "Unoplat Team <team@unoplat.com>",
  "maintainers": [
    {
      "name": "Maintainer One",
      "email": "maintainer1@unoplat.com"
    },
    {
      "name": "Maintainer Two",
      "email": "maintainer2@unoplat.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unoplat/example-library.git"
  },
  "bugs": {
    "url": "https://github.com/unoplat/example-library/issues"
  },
  "homepage": "https://github.com/unoplat/example-library#readme",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "vitest",
    "lint": "eslint src",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0",
    "rollup": "^4.9.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@10.2.3"
}
